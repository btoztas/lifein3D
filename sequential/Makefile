all:
	gcc -std=c99 -Wall -g life3d.c -o life3d
debug:
	gcc -std=c99 -Wall -DDEBUG -g life3d.c -o life3d
iteration:
	gcc -std=c99 -Wall -DITERATION -g life3d.c -o life3d
opt:
	gcc -std=c99 -Wall life3d.c -o life3d
optiter:
	gcc -std=c99 -Wall -DITERATION -o life3d life3d.c
optdebug:
	gcc -std=c99 -Wall -DDEGUG -o life3d life3d.c
